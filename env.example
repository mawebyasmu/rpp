# Environment Variables for Madrasah RPP Wizard
# Copy this file to .env and fill in your values

# Application Configuration
VITE_APP_NAME="Madrasah RPP Wizard"
VITE_APP_VERSION="1.0.0"
VITE_APP_ENVIRONMENT="development"

# Security Configuration
VITE_ENABLE_HTTPS=true
VITE_ENABLE_CORS=true
VITE_MAX_FILE_SIZE=10485760

# API Configuration (if needed in future)
# VITE_API_BASE_URL="https://api.example.com"
# VITE_API_KEY="your-api-key-here"

# Feature Flags
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_DEBUG_MODE=false

# Local Storage Keys (for security)
VITE_STORAGE_PREFIX="madrasah_rpp_"
VITE_SESSION_TIMEOUT=3600000

# Content Security Policy
VITE_CSP_NONCE="your-csp-nonce-here" 